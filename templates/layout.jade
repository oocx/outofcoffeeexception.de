- moment.lang("de")
doctype html
block vars
  - var bodyclass = null;
html(lang='de')
  head
    block head
      include analytics
      meta(charset='utf-8')
      meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
      meta(name='viewport', content='width=device-width')
      title
        block title
          = locals.name
      link(rel='alternate', href=locals.url+'/feed.xml', type='application/rss+xml', title=locals.description)
      link(rel='stylesheet', href='/css/main.css')
      link(rel='stylesheet', href='/css/photoswipe.css')
      link(rel='stylesheet', href='/css/default-skin.css')
      mixin analytics()
  body(class=bodyclass)
    div.content-wrap
      div.content-inner
        header.header
            block header
              div.logo
                h1
                  a(href=locals.url)= locals.name
                p.description= locals.description
        div#content
          block content
            h2 Welcome to zombocom!
        footer
          block footer
            section.about
              !=contents['about.md'].html
            section.copy
              p &copy; #{ new Date().getFullYear() } #{ locals.owner } &mdash; powered by&nbsp;
                a(href='https://github.com/jnordberg/wintersmith') Wintersmith
                //- please leave the "powered by" if you use the design
    include photoswipe
    mixin photoswipe()
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js')
    script(src='/script/photoswipe.min.js')
    script(src='/script/photoswipe-ui-default.min.js')
    script(src='/script/photoswipe-dom.js')
